<bpel:process name="Iberia"
         targetNamespace="http://iberia"
         suppressJoinFailure="yes"
         xmlns:tns="http://iberia"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="IberiaArtifacts.wsdl" namespace="http://iberia" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:Iberia"
                     myRole="IberiaProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:IberiaRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:IberiaResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Iberia.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:Iberia"
                 operation="process" variable="input"
                 createInstance="yes"/>
        <bpel:assign validate="no" name="Assign">
            <bpel:copy>
                <bpel:from><bpel:literal xml:space="preserve"><tns:DetallesVuelo xmlns:tns="http://iberia" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
					  <tns:NumeroVuelo>RPW4079</tns:NumeroVuelo>
					  <tns:Precio>100.0</tns:Precio>
					  <tns:ClasePasaje>Clase turista</tns:ClasePasaje>
					  <tns:HoraSalida>2001-12-31T12:00:00</tns:HoraSalida>
					  <tns:HoraLlegada>2001-12-31T12:00:00</tns:HoraLlegada>
					</tns:DetallesVuelo>
					</bpel:literal>
				</bpel:from>
                <bpel:to variable="output" part="confirmacionVuelo"></bpel:to>
            </bpel:copy>
            
        </bpel:assign>
        
        <!-- Generate reply to synchronous request -->
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:Iberia"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

